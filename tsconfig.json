{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es5", "es6", "es2017"],
    "noImplicitAny": true,
    "moduleResolution": "node",
    "removeComments": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "."
  },
  "exclude": [
    "node_modules"
  ]
}